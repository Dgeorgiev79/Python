count = int(input())

list_numbers = []

for i in range(count):
    number = int(input())
    list_numbers.append(number)

command = input()

list_filtered = []

if command == 'even':
    for i in list_numbers:
        if i % 2 == 0:
            list_filtered.append(i)

elif command == 'odd':
    for i in list_numbers:
        if i % 2 != 0:
            list_filtered.append(i)
elif command == 'negative':
    for i in list_numbers:
        if i < 0:
            list_filtered.append(i)
elif command == 'positive':
    for i in list_numbers:
        if i >= 0:
            list_filtered.append(i)
elif command == 'negative':
    for i in list_numbers:
        if i < 0:
            list_filtered.append(i)

print(list_filtered)
