count = int(input())

count_positives = []
count_negatives = []
n = 0

for i in range(count):
    number = int(input())
    if number >= 0 :
        count_positives.append(number)
        n = n +1
    else:
        count_negatives.append(number)

print(count_positives)
print(n)
print(count_negatives)
print(sum(count_negatives))
